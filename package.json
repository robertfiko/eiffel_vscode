{
  "name": "eiffel-vscode",
  "repository": "https://github.com/robertfiko/eiffel_vscode",
  "displayName": "Eiffel Syntax Highlighting",
  "description": "Bringing Eiffel syntax highlighting to VS Code.",
  "version": "0.0.1",
  "publisher": "robertfiko",
  "icon": "images/logo.png",
  "author": {
    "name": "Robert Fiko"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "eiffel",
        "aliases": [
          "Eiffel",
          "eiffel"
        ],
        "extensions": [
          ".e"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "eiffel",
        "scopeName": "source.eiffel",
        "path": "./syntaxes/Eiffel.json"
      }
    ]
  },
  "devDependencies": {
    "js-yaml": "^4.1.0"
  }
}
